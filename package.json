{
  "name": "stacks-kit",
  "version": "1.1.1",
  "description": "React toolkit for Stacks blockchain. RainbowKit for Bitcoin L2.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "stacks",
    "bitcoin",
    "wallet",
    "react",
    "web3",
    "blockchain",
    "hooks",
    "sbtc"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karkigrishmin/stacks-react.git"
  },
  "bugs": {
    "url": "https://github.com/karkigrishmin/stacks-react/issues"
  },
  "homepage": "https://stacks-kit.vercel.app",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsup && bun run build:css",
    "build:css": "tailwindcss -c tailwind.lib.config.js -i src/styles/library.css -o dist/styles.css --minify",
    "preview": "vite preview",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "lefthook install",
    "prepublishOnly": "bun run build:lib"
  },
  "peerDependencies": {
    "@stacks/connect": "^8.0.0",
    "@stacks/network": "^7.0.0",
    "@stacks/transactions": "^7.0.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@fontsource/geist-mono": "^5.2.7",
    "@fontsource/geist-sans": "^5.2.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stacks/connect": "^8.2.3",
    "@stacks/network": "^7.2.0",
    "@stacks/transactions": "^7.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-path-alias": "^1.0.7",
    "jsdom": "24.1.0",
    "lefthook": "^2.0.11",
    "msw": "^2.12.4",
    "postcss": "^8.5.6",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.10.0",
    "semantic-release": "^25.0.2",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.5.1",
    "typescript": "~5.7.2",
    "vite": "^6.0.7",
    "vitest": "^4.0.15"
  }
}
